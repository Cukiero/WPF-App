<UserControl x:Class="DesktopLaby.Widoki.Widok1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopLaby.Widoki"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Butt1" x:Name="button1" Grid.Row="0" Click="button1_Click"/>
        <TextBox x:Name="textbox1" Grid.Row="1" TextChanged="textbox1_TextChanged" />
        <ComboBox x:Name="combobox1" Grid.Row="2" SelectionChanged="combobox1_SelectionChanged">
            <ComboBox.Items>
                <ComboBoxItem Content="obiekt1"/>
                <ComboBoxItem Content="obiekt2"/>
            </ComboBox.Items>
        </ComboBox>
    </Grid>
</UserControl>
